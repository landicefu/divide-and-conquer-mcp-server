{
  "name": "@landicefu/divide-and-conquer-mcp-server",
  "version": "1.1.0",
  "description": "An MCP server for breaking down complex tasks into manageable pieces with structured JSON storage",
  "main": "build/index.js",
  "type": "module",
  "bin": {
    "divide-and-conquer-mcp-server": "build/index.js"
