    "@types/node": "^20.4.5",
    "typescript": "^5.1.6"
  },
  "files": [
    "build",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/landicefu/divide-and-conquer-mcp-server.git"
  },
